{
    "name": "rayhan/RayMailer",
    "description": "A mailer plugin for CakePHP3 using database driven Layouts and Templates.",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "email", "cron", "preview"],
    "license": "MIT",
    "homepage": "https://github.com/rayhan/RayMailer",
    "authors": [
        {
          "name": "Md. Rayhan Chowdhury",
          "role": "Author"
        }
    ],
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "cakephp/cakephp-codesniffer": "dev-master",
    },
    "autoload": {
        "psr-4": {
            "RayMailer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RayMailer\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
